/*--------------------------------------------------------------------------------------------------
Viridis color palette macro, Shane Rosanbalm, Rho, Inc., 2019

Description:
   This macro generates a set of colors using the viridis color palette. 
      https://cran.r-project.org/web/packages/viridis/vignettes/intro-to-viridis.html
   The user specifies the number of colors wanted and the macro returns the colors as both a macro 
   variable (in a space-separated list) and as a dataset (one obs per color selected). 
   The macro default is to generate viridis colors, but it also contains an option for generating 
   colors from the magma, plasma, inferno, and cividis palettes.

Required parameters:
   n        =  Number of colors to return.
               VALID: integers from 2-256

Optional parmeters:
   palette  =  Palette from which to select the colors.
               VALID: viridis|magma|plasma|inferno|cividis
               DEFAULT: viridis
   greyscale=  Convert colors to greyscale using the luma formula.
               VALID: yes|no
               DEFAULT: no
   reverse  =  Provide colors in reverse order.
               VALID: yes|no
               DEFAULT: no
   out      =  Name of output dataset with one obs per color.
               DEFAULT: work.palette
   result   =  Name of macro variable containing colors as a space-separated list.
               DEFAULT: viridis&n (actually &palette&n)
   debug    =  To debug or not to debug...
               VALID: yes|no
               DEFAULT: no

Example 1:
   %viridis(n=4);

   Result for this call:
      %put &=viridis4;
      viridis4 = cx440154 cx30688E cx35B779 cxFEE724

Example 2:
   %viridis
      (n=7
      ,palette=inferno
      ,reverse=yes
      );

   Result for this call:
      %put &=inferno7;
      inferno7 = cxFDFFA5 cxFCB61A cxED6925 cxBC3754 cx781C6D cx34095F cx000003

--------------------------------------------------------------------------------------------------*/

%macro viridis
      (n=
      ,palette=viridis
      ,greyscale=no
      ,reverse=no
      ,out=palette
      ,result=
      ,debug=no
      ) / minoperator;


   *--------------------------------------------------------------------------------;
   *---------- parameter checking ----------;
   *--------------------------------------------------------------------------------;
   
   %local i;

   %if &n eq %str() %then 
      %put %str(W)ARNING: parameter N is required;
      
   data _null_;
      n = input("&n",??best.);
      if missing(n) 
         or n ne round(n)
         or n > 256
         or n < 2
         then
         put "WAR" "NING: value &=n is either missing, non-integer, >256, or <2";
   run;
   
   %let palette = %upcase(&palette);
   %if not (&palette in (VIRIDIS MAGMA PLASMA INFERNO CIVIDIS)) %then
      %put %str(W)ARNING: unexpected value of &=palette;

   %let greyscale = %upcase(&greyscale);
   %if not (&greyscale in (YES Y NO N)) %then
      %put %str(W)ARNING: unexpected value of &=greyscale;

   %let reverse = %upcase(&reverse);
   %if not (&reverse in (YES Y NO N)) %then
      %put %str(W)ARNING: unexpected value of &=reverse;

   %if &result eq %str() %then
      %let result = &palette&n;
   %global &result;

   %let debug = %upcase(&debug);
   %if &debug in (YES Y) %then
      options mprint; ;

   %put &=n;
   %put &=palette;
   %put &=result;


