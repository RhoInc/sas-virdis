   *--------------------------------------------------------------------------------;
   *---------- create full set of 256 SAS RGB colors ----------;
   *---------- select n= equidistant colors ----------;
   *--------------------------------------------------------------------------------;
   
   *---------- round precise 0-1 values to nearest hex value ----------;
   data _&palette._rgb;
      set _&palette._raw;
      obs = _N_;
      r = 255*input(scan(raw,1,","),best.);
      g = 255*input(scan(raw,2,","),best.);
      b = 255*input(scan(raw,3,","),best.);
      rgb = "cx" || put(r,hex2.) || put(g,hex2.) || put(b,hex2.);
      %if &greyscale in (YES Y) %then %do;
         luma = 0.2126*r + 0.7152*g + 0.0722*b;
         rgb = "cx" || repeat(put(luma,hex2.),3);
      %end;
   run;

   *---------- create output dataset ----------;
   data &out;
      set _&palette._rgb;
      %do i = 1 %to &n;
         if _N_ = 1 + round(255*(&i-1)/(&n-1)) then
            output;
      %end;
   run;

   *---------- reverse output dataset if necessary ----------;
   %if &reverse in (YES Y) %then %do;
      proc sort data=&out out=&out;
         by descending obs;
      run;
   %end;

   *---------- create output macro variable ----------;
   proc sql noprint;
      select   rgb
      into     :&result separated by " "
      from     &out
      ;
   quit;
   %put &result = &&&result;

   *---------- clean up ----------;
   %if &debug in (NO N) %then %do;
      proc datasets library=work;
         delete _viridis_: _magma_: _plasma_: _inferno_: _cividis_:;
      run; quit;
   %end;

%mend viridis;
